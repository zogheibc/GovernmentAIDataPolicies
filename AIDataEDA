            "            gc_ai_strategy_overview          federal_2023_26_data_strategy 0.655777\n",
            "       muncipality_open_data_policy        municipality_data_gov_directive 0.652803\n",
            "           federal_guide_use_gen_ai       provincial_good_practices_gen_AI 0.651296\n",
            "  municipality_digital_data_charter           muncipality_open_data_policy 0.641786\n",
            "  municipality_digital_data_charter        municipality_data_gov_directive 0.574128\n",
            "            gc_ai_strategy_overview                 policy_service_digital 0.540025\n",
            "      federal_2023_26_data_strategy        municipality_data_gov_directive 0.538358\n",
            "      federal_2023_26_data_strategy      municipality_digital_data_charter 0.533600\n"
