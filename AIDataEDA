    {
      "cell_type": "code",
      "source": [
        "# ============================================================\n",
        "# BERTopic (robust, no Transformers): TF-IDF -> SVD embeddings\n",
        "# Includes: Detailed Keyword Weights & Document Probability Grids\n",
        "# ============================================================\n",
        "\n",
        "import os\n",
        "import random\n",
        "import csv\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from pathlib import Path\n",
        "from google.colab import drive\n",
        "\n",
        "# -------------------------\n",
        "# 1. Reproducibility Setup\n",
        "# -------------------------\n",
        "SEED = 42\n",
        "\n",
        "def seed_everything(seed=42):\n",
        "    random.seed(seed)\n",
        "    os.environ['PYTHONHASHSEED'] = str(seed)\n",
        "    np.random.seed(seed)\n",
        "\n",
        "seed_everything(SEED)\n",
        "\n",
        "# -------------------------\n",
        "# 2. Drive Mount & Imports\n",
        "# -------------------------\n",
        "if not os.path.exists('/content/drive'):\n",
        "    drive.mount('/content/drive')\n",
        "\n",
        "# Install required libraries\n",
        "!pip -q install -U \"bertopic==0.16.3\" \"umap-learn==0.5.6\" \"hdbscan==0.8.33\" \"scikit-learn==1.5.1\"\n",
        "\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "from sklearn.decomposition import TruncatedSVD\n",
        "from sklearn.preprocessing import normalize\n",
        "import umap\n",
        "import hdbscan\n",
        "from bertopic import BERTopic\n",
        "\n",
        "# -------------------------\n",
        "# 3. Paths\n",
        "# -------------------------\n",
        "BASE    = Path(\"/content/drive/MyDrive/webscrape_links\")\n",
        "IN_PATH = BASE / \"master_links_with_short_titles.csv\"\n",
        "OUT_DIR = BASE / \"topics\"\n",
        "VIZ_DIR = OUT_DIR / \"viz\"\n",
        "OUT_DIR.mkdir(parents=True, exist_ok=True)\n",
        "VIZ_DIR.mkdir(parents=True, exist_ok=True)\n",
        "\n",
        "# -------------------------\n",
        "# 4. Load data\n",
        "# -------------------------\n",
        "df = pd.read_csv(IN_PATH, dtype=str, keep_default_na=False, quoting=csv.QUOTE_ALL)\n",
        "assert \"doc_id\" in df.columns, \"Missing 'doc_id' in master.\"\n",
        "\n",
        "if \"short_title\" not in df.columns: df[\"short_title\"] = df[\"doc_id\"]\n",
        "if \"jurisdiction\" not in df.columns: df[\"jurisdiction\"] = \"Unknown\"\n",
        "\n",
        "docids = df[\"doc_id\"].tolist()\n",
        "short_titles = df[\"short_title\"].tolist()\n",
        "jurisdictions = df[\"jurisdiction\"].tolist()\n",
        "plot_labels = [f\"[{j}] {s}\" for j, s in zip(jurisdictions, short_titles)]\n",
        "\n",
        "if \"text_clean_tm_ngrams\" in df.columns and df[\"text_clean_tm_ngrams\"].str.strip().any():\n",
        "    SRC_COL = \"text_clean_tm_ngrams\"\n",
        "    token_pattern = r\"(?u)\\b\\w[\\w_]+\\b\"\n",
        "else:\n",
        "    SRC_COL = \"text_clean_tm\"\n",
        "    token_pattern = r\"(?u)\\b\\w+\\b\"\n",
        "\n",
        "docs = df[SRC_COL].fillna(\"\").astype(str).tolist()\n",
        "N = len(docs)\n",
        "\n",
        "# -------------------------\n",
        "# 5. Embeddings & Model\n",
        "# -------------------------\n",
        "vectorizer = TfidfVectorizer(lowercase=False, token_pattern=token_pattern, min_df=1, max_df=0.95)\n",
        "X = vectorizer.fit_transform(docs)\n",
        "\n",
        "svd_dim = int(min(300, max(2, min(X.shape[0] - 1, X.shape[1] - 1))))\n",
        "svd = TruncatedSVD(n_components=svd_dim, random_state=SEED)\n",
        "emb = normalize(svd.fit_transform(X))\n",
        "\n",
        "umap_model = umap.UMAP(n_neighbors=min(15, max(2, N-1)), n_components=min(10, svd_dim), metric=\"cosine\", random_state=SEED)\n",
        "hdbscan_model = hdbscan.HDBSCAN(min_cluster_size=2, min_samples=1, prediction_data=True)\n",
        "\n",
        "topic_model = BERTopic(\n",
        "    umap_model=umap_model,\n",
        "    hdbscan_model=hdbscan_model,\n",
        "    vectorizer_model=vectorizer,\n",
        "    calculate_probabilities=True,\n",
        "    verbose=True,\n",
        ")\n",
        "topics, probs = topic_model.fit_transform(docs, embeddings=emb)\n",
        "\n",
        "# -------------------------\n",
        "# 6. Detailed Outputs (The Breakdown)\n",
        "# -------------------------\n",
        "\n",
        "# A. KEYWORD WEIGHTS BREAKDOWN\n",
        "# Extract top 20 keywords per topic with their c-TF-IDF weights\n",
        "all_topics = topic_model.get_topics()\n",
        "keyword_list = []\n",
        "for topic_num, keywords in all_topics.items():\n",
        "    for word, weight in keywords:\n",
        "        keyword_list.append({\n",
        "            \"topic_id\": topic_num,\n",
        "            \"keyword\": word,\n",
        "            \"weight\": round(weight, 4)\n",
        "        })\n",
        "kw_df = pd.DataFrame(keyword_list)\n",
        "kw_df.to_csv(OUT_DIR / \"bertopic_topic_keywords.csv\", index=False)\n",
        "\n",
        "# B. DOCUMENT PROBABILITY GRID\n",
        "# Create a readable summary of which topics apply to which docs\n",
        "doc_prob_list = []\n",
        "for i in range(N):\n",
        "    # Get indices of topics sorted by probability (highest first)\n",
        "    top_indices = np.argsort(probs[i])[::-1]\n",
        "\n",
        "    doc_entry = {\n",
        "        \"doc_id\": docids[i],\n",
        "        \"jurisdiction\": jurisdictions[i],\n",
        "        \"short_title\": short_titles[i],\n",
        "        \"primary_topic\": top_indices[0],\n",
        "        \"primary_prob\": round(probs[i][top_indices[0]], 4),\n",
        "        \"secondary_topic\": top_indices[1] if len(top_indices) > 1 else -1,\n",
        "        \"secondary_prob\": round(probs[i][top_indices[1]], 4) if len(top_indices) > 1 else 0.0,\n",
        "    }\n",
        "    doc_prob_list.append(doc_entry)\n",
        "\n",
        "prob_summary_df = pd.DataFrame(doc_prob_list)\n",
        "prob_summary_df.to_csv(OUT_DIR / \"bertopic_doc_prob_summary.csv\", index=False)\n",
        "\n",
        "# C. TRADITIONAL OUTPUTS\n",
        "topic_model.get_topic_info().to_csv(OUT_DIR / \"bertopic_topic_info.csv\", index=False)\n",
        "\n",
        "# -------------------------\n",
        "# 7. Visualization\n",
        "# -------------------------\n",
        "plt.figure(figsize=(max(10, 0.6 * probs.shape[1]), max(8, 0.6 * N)))\n",
        "im = plt.imshow(probs, aspect=\"auto\", interpolation=\"nearest\", vmin=0.0, vmax=1.0)\n",
        "plt.xticks(range(probs.shape[1]), [f\"T{t}\" for t in range(probs.shape[1])])\n",
        "plt.yticks(range(N), plot_labels)\n",
        "plt.colorbar(im, label=\"Topic Probability\")\n",
        "plt.title(\"BERTopic • Doc–Topic Probability Grid\")\n",
        "plt.tight_layout()\n",
        "plt.savefig(VIZ_DIR / \"bertopic_doc_topic_heatmap.png\", dpi=150)\n",
        "plt.show()\n",
        "\n",
        "print(f\"Success! Check {OUT_DIR} for 'bertopic_topic_keywords.csv' and 'bertopic_doc_prob_summary.csv'\")"
      ],
      "metadata": {
